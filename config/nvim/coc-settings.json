{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "elixir",
    "eelixir"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "tsserver.formatOnType": false,
  "languageserver": {
    "reason": {
      "command": "~/.config/nvim/reason-lsp.exe",
      "filetypes": [
        "reason"
      ]
    },
    "elixirLS": {
      "command": "~/.config/nvim/elixir-ls/language_server.sh",
      "filetypes": [
        "elixir",
        "eelixir"
      ]
    }
  }
}
